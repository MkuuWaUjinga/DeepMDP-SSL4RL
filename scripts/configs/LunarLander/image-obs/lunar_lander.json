{
    "snapshot_config":{"snapshot_mode": "gap_and_last",
                       "snapshot_gap": 10},
    "env_name": "LunarLander-v2-img",
    "dqn_config": {
        "learning_rate": 0.0001,
        "replay_buffer_size": 400000,
        "buffer_batch_size": 32,
        "n_train_steps": 1,
        "n_epochs": 1,
        "sampler_batch_size": 1,
        "steps_per_epoch": 2000000,
        "min_buffer_size": 64,
        "target_network_update_freq": 10,
        "net": {
            "encoder": {
                "filter_dims": [
                    8,
                    4,
                    3
                ],
                "num_filters": [
                    32,
                    64,
                    64
                ],
                "strides": [
                    4,
                    2,
                    1
                ],
                "dense_sizes": [
                    8
                ],
                "input_shape": [
                    4,
                    84,
                    84
                ]
            },
            "head": {
                "dense_sizes": [
                    256,
                    128
                ]
            },
            "layer_norm": false
        },
        "epsilon_greedy": {
            "episodical_decay": true,
            "exponential_decay_rate": 0.995
        },
        "plots": [
            "episodical_stats",
            "aux_loss_plot",
            "weight_plot",
            "latent_space_correlation_plot"
        ],
        "deepmdp": {
            "use": true,
            "transition_head": {
                "dense_sizes": [
                    512
                ]
            },
            "reward_head": {
                "dense_sizes": []
            }
        }
    }
}
