{
    "snapshot_config": {"snapshot_mode": "gap_and_last",
                       "snapshot_gap": 10},
    "env_name": "LunarLander-v2-stk",
    "dqn_config": {
        "learning_rate": 0.0001,
        "replay_buffer_size": 400000,
        "buffer_batch_size": 32,
        "n_train_steps": 1,
        "n_epochs": 1,
        "sampler_batch_size": 1,
        "steps_per_epoch": 2000000,
        "min_buffer_size": 64,
        "target_network_update_freq": 10,
        "net": {
            "encoder":{
                "input_shape": [32],
                "dense_sizes": [128, 8]
            },
            "head": {
                "dense_sizes": [256, 128]
            },
            "layer_norm": false
        },
        "epsilon_greedy": {
            "exponential_decay_rate": 0.995,
            "episodical_decay": true
        },
        "deepmdp": {
            "use": true,
            "use_linear_transition_net": true,
            "primary_embedding_dim_size": 8
        },
        "plots": ["episodical_stats", "aux_loss_plot", "weight_plot"]
    }
}
