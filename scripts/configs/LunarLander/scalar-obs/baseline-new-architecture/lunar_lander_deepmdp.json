{
    "env_name": "LunarLander-v2-stk",
    "dqn_config": {
        "learning_rate": 0.0001,
        "replay_buffer_size": 100000,
        "buffer_batch_size": 32,
        "n_train_steps": 500,
        "n_epochs": 100,
        "sampler_batch_size": 500,
        "steps_per_epoch": 20,
        "min_buffer_size": 64,
        "target_network_update_freq": 10,
        "net": {
            "encoder":{
                "input_shape": [32],
                "dense_sizes": [128, 8]
            },
            "head": {
                "dense_sizes": [256, 128]
            },
            "layer_norm": false
        },
        "epsilon_greedy": {
            "max_epsilon": 1.0,
            "min_epsilon": 0.05,
            "decay_ratio": 0.1,
            "exponential_decay_rate": 0.995,
            "episodical_decay": true
        },
        "deepmdp": {
            "use": true,
            "use_linear_transition_net": true,
            "primary_embedding_dim_size": 8
        },
        "plots": ["episodical_stats", "aux_loss_plot"]
    }
}